{
  "name": "express-ts-skeleton",
  "version": "1.0.0",
  "description": "My Skeleton App using NodeJs and ExpressJS with typescript",
  "main": "./src/app.ts",
  "scripts": {
    "start": "npm run build && node ./build/app.js",
    "dev": "npx nodemon",
    "build": "rimraf ./build && tsc -p .",
    "compile-sass": "sass --watch src/public/scss:src/public/css"
  },
  "keywords": [
    "nodejs"
  ],
  "author": "anggi",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.4.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "moment": "^2.29.4",
    "mongoose": "^7.4.2",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1"
  }
}
